# NODE_ENV=production
NODE_ENV=development
PORT=3000

#When application is running in a container should use the external IP for keycloak
AUTH_SERVER_URL=https://ip-address:30524/auth/
# AUTH_SERVER_URL=http://172.18.32.1:30080/auth/
# AUTH_SERVER_URL=http://172.17.208.1:8080/auth/
#using the localhost for keycloak does NOT work when application is running in a container - Access denied
# AUTH_SERVER_URL=http://localhost:8080/auth/

AUTH_REALM=myrealm
AUTH_CLIENT=vanilla
# none or external
AUTH_SSL_REQUIRED=external
# Normally public key is not required 
# AUTH_PUBLIC_

#Role type: client or realm. Defaults to client
AUTH_ROLE_TYPE=client
AUTH_USER_ROLE=user
AUTH_ADMIN_ROLE=admin

NODE_TLS_REJECT_UNAUTHORIZED=0

# DEBUG=memorystore